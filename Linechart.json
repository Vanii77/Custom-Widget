{
    "eula": "",
    "vendor": "SAP",
    "license": "",
    "id": "line.simple",
    "version": "0.0.1",
    "name": "ECharts Line Chart Hosted SAP Github",
    "newInstancePrefix": "EChartLineChart",
    "description": "A sample custom widget Line Chart Hosted in SAP Github",
    "webcomponents": [
      {
        "kind": "main",
        "tag": "line-simple",
        "url": "https://github.com/Vanii77/Custom-Widget/blob/main/Linechart.js",
        "integrity": "",
        "ignoreIntegrity": true
      }
    ],
    "properties": {
          "width": {
              "type": "integer",
              "default": 600
          },
          "height": {
              "type": "integer",
              "default": 420
          },
      "dimensionFeed": {
              "type": "string[]",
              "default": []
          },
          "measureFeed": {
              "type": "string[]",
              "default": []
          },
      "caption": {
            "type": "string",
            "description": "",
            "default": ""
        }
    },
    "dataBindings": {
          "myDataSource": {
              "feeds": [
                  {
            "id": "dimensions",
            "description": "Dimensions",
            "type": "dimension"
          },
          {
            "id": "measures",
            "description": "Measures",
            "type": "mainStructureMember"
          }
              ]
          }
      },
    "methods": {
      "getCaption": {
        "returnType": "string",
        "description": "The caption of the bar chart",
        "parameters": [],
        "body": "return this.caption;"
      },
      "setCaption": {
        "description": "The caption of the bar chart",
        "parameters": [
          {
            "name": "caption",
            "type": "string",
            "description": "The new caption"
          }
        ],
        "body": "this.caption = caption;"
      },
      "openSelectModelDialog": {
        "parameters": [],
        "body": "this.dataBindings.getDataBinding('myDataSource').openSelectModelDialog();"
      },
      "getDimensions": {
        "parameters": [],
        "body": "return this.dataBindings.getDataBinding('myDataSource').getDataSource().getDimensions();",
        "returnType": "DimensionInfo[]"
      },
      "getMeasures": {
        "parameters": [],
        "body": "return this.dataBindings.getDataBinding('myDataSource').getDataSource().getMeasures();",
        "returnType": "MeasureInfo[]"
      },
      "addDimension": {
        "parameters": [
          {
            "name": "dimensionId",
            "type": "string"
          }
        ]
      },
      "addMeasure": {
        "parameters": [
          {
            "name": "measureId",
            "type": "string"
          }
        ]
      },
      "removeDimension": {
        "parameters": [
          {
            "name": "dimensionId",
            "type": "string"
          }
        ]
      },
      "removeMeasure": {
        "parameters": [
          {
            "name": "measureId",
            "type": "string"
          }
        ]
      },
      "getDimensionsOnFeed": {
        "parameters": [],
        "returnType": "string[]"
      },
      "getMeasuresOnFeed": {
        "parameters": [],
        "returnType": "string[]"
      },
      "getDataSource": {
        "parameters": [],
        "body": "return this.dataBindings.getDataBinding('myDataSource').getDataSource();",
        "returnType": "DataSource"
      },
      "setModel": {
        "parameters": [
          {
            "name": "modelId",
            "type": "string"
          }
        ],
        "body": "this.dataBindings.getDataBinding('myDataSource').setModel(modelId);"
      }
  
    },
    "events": {
      "onResultChanged": {
        "description": "called when the result set of the widget changes"
      },
      "onClick": {
        "description": "called when the result set of the widget changes"
      }
  }}
